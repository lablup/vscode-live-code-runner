{
    "name": "live-code-runner",
    "displayName": "live-code-runner",
    "description": "Run code snippets via Sorna Cloud API server (https://cloud.sorna.io)",
    "version": "0.0.1",
    "publisher": "lablup",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.runCode"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.runCode",
            "title": "Run Code On Sorna"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}