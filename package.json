{
    "name": "live-code-runner",
    "displayName": "Live Code Runner",
    "description": "Run your codes via Sorna Cloud API server (https://cloud.sorna.io)",
    "version": "1.2.0",
    "icon": "images/live-code-runner-logo.png",
    "publisher": "lablup",
    "license": "See LICENSE in LICENSE file",
    "bugs": {
        "url": "https://github.com/lablup/vscode-live-code-runner/issues"
    },
    "homepage": "https://github.com/lablup/vscode-live-code-runner/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/lablup/vscode-live-code-runner.git"
    },
    "categories": [
        "Languages",
        "Debuggers",
        "Other"
    ],
    "engines": {
        "vscode": "^1.9.0"
    },
    "keywords": [
        "python",
        "javascript",
        "tensorflow",
        "language",
        "cloud"
    ],
    "main": "./out/src/extension",
    "activationEvents": [
        "onCommand:live-code-runner.runCode"
    ],
    "contributes": {
        "commands": [
            {
                "command": "live-code-runner.runCode",
                "title": "Run Code On Lablup.AI Cloud"
            }
        ],
        "keybindings": [
            {
                "command": "live-code-runner.runCode",
                "key": "ctrl+alt+r"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "!inOutput",
                    "command": "live-code-runner.runCode",
                    "group": "navigation"
                }
            ]
        },
        "languages": [
            {
                "id": "live-code-runner-console",
                "mimetypes": [
                    "text/x-code-output"
                ]
            }
        ],
        "grammars": [
            {
                "language": "live-code-runner-console",
                "scopeName": "live-code-runner.console",
                "path": "./syntaxes/console.tmLanguage"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Live code runner",
            "properties": {
                "live-code-runner.accessKey": {
                    "type": "string",
                    "default": "",
                    "description": "Access API key generated at cloud.sorna.io."
                },
                "live-code-runner.secretKey": {
                    "type": "string",
                    "default": "",
                    "description": "Secret API key generated at cloud.sorna.io."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "punycode": "^2.1.0",
        "node-fetch": "^1.6.3"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "punycode": "^2.1.0",
        "node-fetch": "^1.6.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
